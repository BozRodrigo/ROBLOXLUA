	local function sendWebhook(Blehh)
		local everyone
		if getgenv().ping == "" then
			everyone = ""
		elseif getgenv().ping ~= "" then
			everyone = "<@"..getgenv().ping..">"
		end
		local data = {
			['content'] = everyone,
			['username'] = "HsB CatHook",
			["embeds"] = {{
				['title'] = "Your Bizarre Adventure",
				['color'] = tonumber(0xff7f42),
				["footer"] = {
					["text"] = ("HsB Hub ("..os.date("%H:%M")..")"),
					["icon_url"] = "https://cdn.discordapp.com/attachments/875211210486870076/1067422518413103134/hsb222.png"
				},
				["fields"] = {
					{
						["name"] = "Account Name", -- This is  the title of your field.
						["value"] = "||"..game.Players.LocalPlayer.Name.."||" -- This is the value of your field.
					},
					{
						["name"] = "InfoHook", -- This is  the title of your field.
						["value"] = Blehh -- This is the value of your field.
					},
				}}
			}
		}
		local newdata = game:GetService("HttpService"):JSONEncode(data)

		local headers = {
			["content-type"] = "application/json"
		}
		request = http_request or request or HttpPost or syn.request
		local abcdef = {Url = getgenv().webhooklink, Body = newdata, Method = "POST", Headers = headers}
		request(abcdef)
	end

	local function rasa(itemname)
		local everyone
		if itemname == "Lucky Arrow" then
			everyone = "@everyone"
		else 
			if getgenv().ping == "" then
				everyone = "@everyone"
			elseif getgenv().ping ~= "" then
				everyone = "<@"..getgenv().ping..">"
			end

		end
		local amount = 0
		for _, child in pairs (game.Players.LocalPlayer.Backpack:GetChildren()) do
			if child.Name == itemname then
				amount = amount + 1
			end
		end
		local data = {
			['content'] = everyone,
			['username'] = "HsB CatHook",
			["embeds"] = {{
				['title'] = "Your Bizarre Adventure",
				['color'] = tonumber(0xff7f42),
				["footer"] = {
					["text"] = ("HsB Hub ("..os.date("%H:%M")..")"),
					["icon_url"] = "https://cdn.discordapp.com/attachments/875211210486870076/1067422518413103134/hsb222.png"
				},
				["fields"] = {
					{
						["name"] = "Name", -- This is  the title of your field.
						["value"] = "||"..game.Players.LocalPlayer.Name.."||" -- This is the value of your field.
					},
					{
						["name"] = "Item", -- This is  the title of your field.
						["value"] = ""..itemname.."" -- This is the value of your field.
					},
					{
						["name"] = "Current Amount of " ..itemname.. "", -- This is  the title of your field.
						["value"] = amount -- This is the value of your field.   
					}
				}}
			}
		}
		local newdata = game:GetService("HttpService"):JSONEncode(data)

		local headers = {
			["content-type"] = "application/json"
		}
		request = http_request or request or HttpPost or syn.request
		local abcdef = {Url = webhooklink, Body = newdata, Method = "POST", Headers = headers}
		request(abcdef)
	end

