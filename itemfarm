	mold = hookmetamethod(game,"__namecall", function(self,...)
		local args = {...}
		local method = getnamecallmethod()
		if tostring(method) == "InvokeServer" and args[1]=="ToggleStand" then
			return wait(9e9)
		end
		return mold(self, ...)
	end)

